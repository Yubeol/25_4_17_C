//이름 정수 입력 => 이름 등급(abcdf)출력
#include <stdio.h>

int main(void) {

	int score;
	char name[11];
	char grade;

	printf("이름 입력: ");
	scanf_s("%s", name);

	printf("점수 입력: ");
	scanf_s("%d", &score);

		if (score >= 90) {
			grade = 'A';
		}
		else if (score >= 80) {
			grade = 'B';
	}
		else if (score >= 70) {
			grade = 'C';
		}
		else if (score >= 60) {
			grade = 'D';
		}
		else {
			grade = 'F';
		}
		printf("%s님의 등급은 %c입니다", name, grade);

	return 0;
}
